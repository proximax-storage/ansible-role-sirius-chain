{% set combined_ext_recovery_config = default_ext_recovery_config | combine(ext_recovery_config, recursive=true) -%}
[extensions]

# addressextraction must be first because mongo and zeromq depend on extracted addresses
extension.addressextraction = {{ combined_ext_recovery_config['extension_addressextraction'] | lower }}
extension.mongo = {{ combined_ext_recovery_config['extension_mongo'] | lower }}
extension.zeromq = {{ combined_ext_recovery_config['extension_zeromq'] | lower }}

extension.hashcache = {{ combined_ext_recovery_config['extension_hashcache'] | lower }}